while (i < fdf->hght * fdf->wdth)
	{
		ft_putnbr((fdf->cur)[i]->x);
		ft_putchar(' ');
		ft_putnbr((fdf->cur)[i]->y);
		ft_putchar(';');
		i++;
		if (i % fdf->wdth == 0)
			ft_putchar('\n');
}

